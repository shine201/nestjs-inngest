{
  "name": "nestjs-inngest",
  "version": "1.1.3",
  "description": "Advanced NestJS integration for Inngest with performance optimizations, type safety, and enterprise features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test && npm run typecheck"
  },
  "keywords": [
    "nestjs",
    "inngest",
    "events",
    "background-jobs",
    "typescript",
    "microservices",
    "event-driven",
    "serverless",
    "performance",
    "enterprise",
    "queue",
    "workflow",
    "saga",
    "circuit-breaker",
    "distributed-systems"
  ],
  "author": "NestJS Inngest Contributors",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "inngest": "^3.40.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.5.0"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/testing": "^11.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "jest": "^29.0.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.0.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shine201/nestjs-inngest.git"
  },
  "bugs": {
    "url": "https://github.com/shine201/nestjs-inngest/issues"
  },
  "homepage": "https://github.com/shine201/nestjs-inngest#readme"
}
